  }, {
    "comment" : "WM add",
    "regex" : "(=>WM:\\ .*)",
    "components" : [ "blue" ],
    "enabled" : true
  }, {
    "comment" : "WM remove",
    "regex" : "(<=WM:\\ .*)",
    "components" : [ "orange" ],
    "enabled" : true
  }, {
    "comment" : "rule excise",
    "regex" : "(#+)",
    "components" : [ "red" ],
