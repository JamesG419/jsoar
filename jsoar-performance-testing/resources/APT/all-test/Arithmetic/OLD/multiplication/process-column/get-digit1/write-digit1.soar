sp {process-column*propose*write-digit1
   (state <s> ^name get-digit1
              ^result <a>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name write-digit1)
}

sp {process-column*apply*write-digit1
   (state <s> ^operator.name write-digit1
              ^result <a>
              ^carry-borrow <c> 
              ^superstate <ss>)
-->
   (<ss> ^digit1 <a>
         ^carry-borrow <c>)
}

