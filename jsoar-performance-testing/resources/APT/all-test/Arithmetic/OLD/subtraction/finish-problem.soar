sp {subtraction*propose*finish-problem
   (state <s> ^name subtraction
              ^subtraction-problem.current-column <c>)
   (<c> ^difference
        ^next-column nil)
-->
   (<s> ^operator <op> + =)
   (<op> ^name finish-problem)
}

sp {subtraction*apply*finish-problem*1column*difference
   (state <s> ^name subtraction
              ^operator.name finish-problem
              ^subtraction-problem <ap>)
   (<ap> ^one-column <oc>)
   (<oc> ^sub1 <n1>
         ^sub2 <n2>
         ^difference <n3>
         ^next-column nil)
-->
   (write (crlf) | | <n1> )
   (write (crlf) |-| <n2> )
   (write (crlf) |--|)
   (write (crlf) | | <n3> )
   (halt)
}
sp {subtraction*apply*finish-problem*2column*difference
   (state <s> ^name subtraction
              ^operator.name finish-problem
              ^subtraction-problem <ap>)
   (<ap> ^one-column <oc>)
   (<oc> ^sub1 <n1>
         ^sub2 <n2>
         ^difference <a1>
         ^next-column <tc>)
   (<tc> ^sub1 <n3>
         ^sub2 <n4>
         ^difference <a2>
         ^next-column nil)
-->
   (write (crlf) | | <n3> <n1> )
   (write (crlf) |-| <n4> <n2> )
   (write (crlf) |---|)
   (write (crlf) | | <a2> <a1>)
   (halt)
}
sp {subtraction*apply*finish-problem3*column*difference
   (state <s> ^name subtraction
              ^operator.name finish-problem
              ^subtraction-problem <ap>)
   (<ap> ^one-column <oc>)
   (<oc> ^sub1 <n1>
         ^sub2 <n2>
         ^difference <a1>
         ^next-column <tc>)
