sp {sub-column*propose*write-sub1
   (state <s> ^name get-sub1
              ^difference <a>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name write-sub1)
}

sp {sub-column*apply*write-sub1
   (state <s> ^operator.name write-sub1
              ^difference <a>
              ^borrow <c> 
              ^superstate <ss>)
-->
   (<ss> ^sub1 <a>
         ^borrow <c>)
}

