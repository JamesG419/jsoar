sp {sub-column*propose*get-sub1
   (state <s> ^name sub-column
             -^sub1)
-->
   (<s> ^operator <op> + =)
   (<op> ^name get-sub1)
}

sp {sub-column*apply*get-sub1
   (state <s> ^operator.name get-sub1
              ^current-column <cc>)
   (<cc> ^sub1 <a>
        -^borrow 1)
-->
   (<s> ^sub1 <a>)
}


